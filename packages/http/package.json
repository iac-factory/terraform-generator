{
    "name": "tf-http",
    "private": false,
    "version": "0.6.2",
    "type": "module",
    "types": "index.d.ts",
    "description": "Terraform JSON Schema Utility",
    "license": "BSD-2-Clause",
    "author": "Jacob B. Sanders <jacob.sanders@cloudhybrid.io>",
    "main": "index.js",
    "scripts": {
        "prestart": "tsc",
        "start": "ts-node -r index.ts .",
        "build": "tsc --pretty",
        "watch": "tsc --watch",
        "upload": "tsc && npm publish --access public",
        "link": "npm i && tsc && npm link"
    },
    "dependencies": {
        "inquirer": "^8.2.2",
        "ts-node": "^10.7.0"
    },
    "devDependencies": {
        "@swc/core": "^1.2.148",
        "@tsconfig/node16-strictest-esm": "^1.0.0",
        "@tsconfig/recommended": "^1.0.1",
        "@tsconfig/strictest": "^1.0.0",
        "@types/inquirer": "^8.2.0",
        "@types/node": "^17.0.19",
        "ts-node": "^10.7.0",
        "tsc-watch": "^4.6.2",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.2"
    },
    "keywords": [
        "terraform",
        "tf-http",
        "automation",
        "code-generation",
        "backend",
        "http-backend",
        "npm",
        "node.js"
    ],
    "bin": {
        "tf-http": "index.js"
    }
}
